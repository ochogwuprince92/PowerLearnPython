class Vehicle:
    def move(self):
        pass  # Generic method to be overridden

class Car(Vehicle):
    def move(self):
        print("Driving ğŸš—")

class Plane(Vehicle):
    def move(self):
        print("Flying âœˆï¸")

class Bicycle(Vehicle):
    def move(self):
        print("Pedaling ğŸš²")

# Polymorphism in action
vehicles = [Car(), Plane(), Bicycle()]

for v in vehicles:
    v.move()
